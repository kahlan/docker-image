FROM composer/composer:php5-alpine


# Install Kahlan
RUN mkdir -p /usr/src/kahlan \
 && cd /usr/src/kahlan \
 && composer selfupdate \
 && composer require "kahlan/kahlan:3.0.2" \
 && ln -s /usr/src/kahlan/vendor/bin/kahlan /usr/local/bin/kahlan \
 && ln -s /usr/local/bin/kahlan /kahlan




